{
  "name": "ack-angular-webcam",
  "version": "1.3.2",
  "description": "Angular2 webcam component. Based on MediaDevices and getUserMedia.js ",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "ack-reload -d ./example/www -b",
    "start:old": "webpack-dev-server --open --config webpack.config.js --hot --progress --colors --port 3000",
    "test:old": "karma start",
    "e2e": "./node_modules/protractor/bin/webdriver-manager update && protractor",
    "watch:js": "ack-webpack ./example/main.ts example/www/index.js --project example/tsconfig.json --modules . --watch --browser",
    "watch:dist": "watch 'npm-run-all build:dist compile:aot' lib/",
    "watch": "npm run watch:dist & npm run watch:js",
    "compile:aot": "npm-run-all copy:example-dist compile:aot:ts",
    "compile:aot:ts": "ngc -p example/tsconfig.aot.json",
    "copy:example-dist": "ncp ./dist example/app/ack-angular-webcam",
    "build:dist": "ngc --declaration --project lib",
    "ack-webpack": "ack-webpack",
    "build": "npm-run-all compile:dist:package build:dist compile:aot build:js",
    "build:js": "ack-webpack example/index.aot.ts example/www/index.js --production --project example/tsconfig.json",
    "compile:dist:package": "node scripts/update-dist-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ackerapple/ack-angular-webcam.git"
  },
  "keywords": [
    "ng2",
    "angular2",
    "getusermedia",
    "webcam",
    "video",
    "gum"
  ],
  "author": "Akcer Apple",
  "licenses": [
    {
      "type": "MIT"
    }
  ],
  "devDependencies": {
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "ack-webpack": "^1.1.36",
    "core-js": "^2.4.1",
    "minimatch": "^3.0.4",
    "npm-run-all": "^4.0.2",
    "reflect-metadata": "0.1.8",
    "rxjs": "^5.4.0",
    "ts-loader": "^2.2.2",
    "typescript": "^2.4.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.14.0",
    "zone.js": "^0.8.10"
  },
  "bugs": {
    "url": "https://github.com/ackerapple/ack-angular-webcam/issues"
  },
  "homepage": "https://github.com/ackerapple/ack-angular-webcam#readme",
  "directories": {
    "example": "example"
  },
  "dependencies": {},
  "license": "MIT",
  "private": true
}
